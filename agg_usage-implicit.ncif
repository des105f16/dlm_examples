principal a;

typedef struct t_usage {
  int {{a->a}} start_time;
  int {{a->a}} usageWh;
} t_usage;

int {{udata}} agg_usage(t_usage *udata, int {{_}} udata_size) {
  int sum = 0; // ⊥
  int i = 0; // ⊥

  while (i < udata_size) { // i ⊔ udata_size = L1
    sum = sum + udata[i].usageWh; // sum ⊔ udata ⊔ udata.usageWh ⊔ i ⊔ L1 ⊑ sum
  }

  agg_usage -->? a {
    return <|sum|>; // sum ⊑ Ld ⊔ {{a->_}} | Ld ⊑ udata
  }
  return -1; // ⊥ ⊑ udata
}
